				float2 texCoord43 = IN.ase_texcoord7.xy * float2( 1,1 ) + float2( 0,0 );
				float2 panner46 = ( ( _TimeParameters.x * 0.05 ) * _NoiseSpeed + texCoord43);
				float simplePerlin2D41 = snoise( panner46*3.0 );
				simplePerlin2D41 = simplePerlin2D41*0.5 + 0.5;
				float time31 = 0.0;
				float2 texCoord35 = IN.ase_texcoord7.xy * float2( 1,1 ) + float2( 0,0 );
				float2 panner38 = ( ( _TimeParameters.x * 0.05 ) * _VoronoiSpeed + texCoord35);
